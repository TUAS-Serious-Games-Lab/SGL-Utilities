{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SGL.Analytics.Client/bin/*/*/SGL.Analytics.Client.dll",
            "SGL.Analytics.Backend.Logs.Collector/bin/*/*/SGL.Analytics.Backend.Logs.Collector.dll",
            "SGL.Analytics.Backend.Users.Registration/bin/*/*/SGL.Analytics.Backend.Users.Registration.dll"
          ],
		  "src" : ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
	  {
		"files" :[
			"README.md"
		],
		"src" : ".."
	  },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docfx_output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default","statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}