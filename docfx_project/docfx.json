{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "SGL.Utilities/bin/Debug/*/SGL.Utilities.dll",
            "SGL.Utilities.Backend/bin/Debug/*/SGL.Utilities.Backend.dll",
            "SGL.Utilities.Backend.AspNetCore/bin/Debug/*/SGL.Utilities.Backend.AspNetCore.dll",
            "SGL.Utilities.Backend.Security/bin/Debug/*/SGL.Utilities.Backend.Security.dll",
            "SGL.Utilities.Backend.TestUtilities/bin/Debug/*/SGL.Utilities.Backend.TestUtilities.dll",
            "SGL.Utilities.Crypto/bin/Debug/*/SGL.Utilities.Crypto.dll",
            "SGL.Utilities.Logging/bin/Debug/*/SGL.Utilities.Logging.dll",
            "SGL.Utilities.PrometheusNet/bin/Debug/*/SGL.Utilities.PrometheusNet.dll",
            "SGL.Utilities.TestUtilities.XUnit/bin/Debug/*/SGL.Utilities.TestUtilities.XUnit.dll"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/**.md",
          "docs/**/toc.yml",
          "SGL.Utilities.*/toc.yml",
          "SGL.Utilities.*/index.md",
          "index.md"
        ],
        "src": ".."
      },
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../docfx_output",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "statictoc",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}